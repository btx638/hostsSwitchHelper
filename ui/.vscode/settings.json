{
    "eslint.options": {
        "plugins": ["vue"]
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "html",
            "autoFix": true
          },
          {
            "language": "vue",
            "autoFix": true
          }
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
}